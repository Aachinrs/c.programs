#include<stdio.h>
int main()
{
	int n,i=1;
	unsigned long long int factorial=1;
	printf("enter a number:");
	scanf("%d",&n);
	if (n<0)
	{
		printf("factorial doesnt apply for negative numbers");
	}
	else
	{
		for(i=1;i<=n;i++)
		{
			factorial*=i;
		}
	}
	printf("factorial of %d=%llu",n,factorial);
	return 0;
}
